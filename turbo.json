{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ]
    },
    "e2e": {
      "dependsOn": [
        "^e2e"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "ci-setup": {
      "dependsOn": [
        "^ci-setup"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
